!! urxvt settings
URxvt*scrollBar: false
URxvt*secondaryScroll: true
URxvt*urlLauncher: chromium
URxvt*depth: 32
URxvt*internalBorder: 0
URxvt*saveLines: 8192
URxvt*transparent: true
URxvt*shading: 15

URxvt*background:  #212121
URxvt*foreground:  #f0f0f0
URxvt*cursorColor: #f0f0f0

URxvt*color0:      #2e3436
URxvt*color1:      #cc0000
URxvt*color2:      #4e9a06
URxvt*color3:      #c4a000
URxvt*color4:      #3465a4
URxvt*color5:      #5c3566
URxvt*color6:      #16989A
URxvt*color7:      #D3D7CF
URxvt*color8:      #555753
URxvt*color9:      #EF2929
URxvt*color10:     #8AE234
URxvt*color11:     #FCE94F
URxvt*color12:     #729FCF
URxvt*color13:     #75507b

URxvt*font:               xft:Terminus:style=Semibold:pixelsize=12:antialias=true:hinting=slight
URxvt*boldFont:           xft:Terminus:style=bold:pixelsize=12:antialias=true:hinting=slight

!! xft settings
Xft.dpi      : 96
Xft.antialias: true
Xft.rgba     : rgb
Xft.hinting  : true
Xft.hintstyle: hintslight
